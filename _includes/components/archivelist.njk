<section class="archive">
	{% for post in postslist | reverse %}
    <div class="archive__item">
      <a href="{{post.url}}" class="archive__item-link">
        <img class="archive__item-image" src="{{post.data.comicPageUrl}}"/>
        <h3 class="archive__item-title">{{post.data.title}}</h3>
      </a>
    </div>
	{% endfor %}
</section>
